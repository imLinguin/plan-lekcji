{
  "name": "ft-plan-lekcji",
  "productName": "Plan Lekcji Elektronik",
  "version": "1.0.0",
  "description": "Frontend na projekt na wystawę prac ucznów.",
  "main": "main.js",
  "scripts": {
    "dev": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dist": "electron-builder -wl"
  },
  "author": "Linguin & Olek Baran",
  "homepage": "https://github.com/imLinguin/plan-lekcji",
  "license": "Apache-2.0 License",
  "devDependencies": {
    "electron": "^11.1.1",
    "electron-builder": "^22.9.1"
  },
  "build": {
    "win": {
      "target": [
        "nsis"
      ],
      "artifactName": "${productName}-win-${version}.${ext}",
      "icon": "content/images/logo.png"
    },
    "nsis": {
      "deleteAppDataOnUninstall": true
    },
    "linux": {
      "target": [
        "deb"
      ],
      "maintainer": "Linguin & Olek Baran"
    }
  }
}
